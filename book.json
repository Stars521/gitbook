	"pluginsConfig": {
        "github": {
            "url":"https://github.com/Stars521"
        },
		 "anchor-navigation-ex": {
            "showLevel":false,
            "showGoTop":false
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "donate": {
            "wechat": "微信收款的二维码URL",
            "alipay": "支付宝收款的二维码URL",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
		 },
        "sharing": {
            "google": true,
            "qq": true,
            "qzone": true,
            "weibo": true,
            "all": [
               "google","weibo","qq", "qzone"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy mine 2020-2023",
            "modify_label": "文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
