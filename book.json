    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "person.md"
    },
    "links": {
        "sidebar": {
            "我的博客": "https://blog.csdn.net/Stars_min?spm=1001.2101.3001.5113",
            "我的GitHub": "https://github.com/Stars521"
        }       
    },
	"plugins": [
        "-sharing",
		"local-video",
        "sharing-plus",
        "github",
        "code",
        "expandable-chapters",
        "back-to-top-button",
        "-lunr",
        "-search",
        "search-pro",
        "tbfed-pagefooter",
        "page-toc-button",
        "donate",
        "pageview-count",
        "hide-element",
        "anchor-navigation-ex"
